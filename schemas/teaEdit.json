{
  "$schema": "http://json-schema.org/draft-01/schema#",
  "$id": "http://teaTime.com/teaEdit.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1
    },
    "brand": {
      "type": "string",
      "minLength": 1
    },
    "description": {
      "type": "string",
      "minLength": 1
    },
    "category": {
      "type": ["string", "null"]
    },
    "review": {
      "type": ["string", "null"]
    },
    "country_of_origin": {
      "type": ["string", "null"]
    },
    "organic": {
      "type": ["boolean", "null"]
    },
    "img_url": {
      "type": ["string", "null"],
      "format": "uri"
    },
    "brew_time": {
      "type": ["integer", "null"]
    },
    "brew_temp": {
      "type": ["integer", "null"]
    }
  },
  "additionalProperties": false,
  "required": []
}
